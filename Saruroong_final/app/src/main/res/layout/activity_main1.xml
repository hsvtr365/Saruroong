<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f9f5ea"
    android:id="@+id/container_main">

    <ImageView
        android:id="@+id/GBdiaryBtn1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginBottom="67dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="100dp"
        android:background="@android:color/transparent"
        android:src="@drawable/gbdiary"
        app:layout_constraintBottom_toTopOf="@+id/PlaylistBtn1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/MessageBtn1"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginBottom="67dp"
        android:layout_marginEnd="24dp"
        android:layout_marginTop="100dp"
        android:background="@android:color/transparent"
        android:src="@drawable/letter"
        app:layout_constraintBottom_toTopOf="@+id/CookieBtn1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/PlaylistBtn1"
        android:layout_width="152dp"
        android:layout_height="150dp"
        android:layout_marginBottom="100dp"
        android:layout_marginStart="24dp"
        android:background="@android:color/transparent"
        android:src="@drawable/playlist"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/CookieBtn1"
        android:layout_width="152dp"
        android:layout_height="150dp"
        android:layout_marginBottom="100dp"
        android:layout_marginEnd="24dp"
        android:background="@android:color/transparent"
        android:src="@drawable/cookie_cate"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</android.support.constraint.ConstraintLayout>
